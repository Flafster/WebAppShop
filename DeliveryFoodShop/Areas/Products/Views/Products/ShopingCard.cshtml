@{
}


<table class="table">
    @foreach(var item in Model){
    <tr>
        <th>
            <div class="d-flex justify-content-center">
                <img src="@item.Image" style="width:50%">
            </div>
        </th>
        <td>@item.Description</td>
        <td>@item.Category.Name</td>
        <td>@item.Price $</td>

    
    </tr>
    }
</table>